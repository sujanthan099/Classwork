#include <stdio.h>

int main() {
    int n, i, odd = 0;
    printf("Enter the size of the array: ");
    scanf("%d", &n);
    int arr[n];
    printf("Enter the elements of the array: ");
    for(i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
        odd ^= arr[i];
    }
    printf("The odd element is %d\n", odd);
    return 0;
}
